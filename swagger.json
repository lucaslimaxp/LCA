  "tags": [
    {
      "name": "LCI",
      "description": "Lifecycle Inventory"
    }
  ],
  "schemes": [
    "https",
    "http"
  ],
  "paths": {
    "/lcarun": {
      "post": {
        "tags": [
          "lci"
        ],
        "summary": "Runs LCI from given input",
        "description": "",
        "operationId": "runLCI",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "in": "body",
            "name": "params",
            "schema": {
              "type": "object",
              "required": [
                "biomass",
                "grindfuel",
                "excavatfuel",
                "distance"
              ],
              "properties": {
                "biomass": {
                  "type": "number",
                  "example": 1.316891466
                },
                "grindfuel": {
                  "type": "number",
                  "example": 0.002368841
                },
                "excavatfuel": {
                  "type": "number",
                  "example": 0.000640885
                },
                "distance": {
                  "type": "number",
                  "example": 101
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "$ref": "#/definitions/LciOutput"
            }
          },
          "400": {
            "description": "Invalid parameters supplied"
          }
        }
      }
    },
    "/lci/{group}": {
      "get": {
        "tags": [
          "lci"
        ],
        "summary": "Runs LCI from given input",
        "description": "",
        "operationId": "getGroupSums",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "biomass",
            "in": "query",
            "required": true,
            "type": "number"
          },
          {
            "name": "grindfuel",
            "in": "query",
            "required": true,
            "type": "number"
          },
          {
            "name": "excavatfuel",
            "in": "query",
            "required": true,
            "type": "number"
          },
          {
            "name": "distance",
            "in": "query",
            "required": true,
            "type": "number"
          },
          {
            "name": "group",
            "in": "path",
            "required": true,
            "type": "string",
            "enum": [
              "renewable",
              "nonrenewable",
              "water"
            ]
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "$ref": "#/definitions/LciOutput"
            }
          },
          "400": {
            "description": "Invalid parameters supplied"
          }
        }
      }
    }
  },
  "definitions": {
    "Lci": {
      "type": "object",
      "properties": {
        "oid": {
          "type": "integer",
          "format": "int64"
        },
        "flow": {
          "type": "string",
          "description": "Name of Flow"
        },
        "unit": {
          "type": "string"
        },
        "diesel": {
          "type": "number",
          "format": "float"
        },
        "transport": {
          "type": "number",
          "format": "float"
        },
        "electricity": {
          "type": "number",
          "format": "float"
        },
        "lci_group": {
          "type": "string",
          "enum": [
            "renewable",
            "nonrenewable",
            "water"
          ]
        }
      },
      "xml": {
        "name": "Lci"
      }
    },
    "LciConsumptionOutput": {
      "type": "object",
      "properties": {
        "renewableSum": {
          "type": "number",
          "format": "float",
          "description": "Sum of Renewable Group"
        },
        "nonrenewableSum": {
          "type": "number",
          "format": "float",
          "description": "Sum of Nonrenewable Group"
        },
        "waterSum": {
          "type": "number",
          "format": "float",
          "description": "Sum of Water Group"
        }
      },
      "xml": {
        "name": "LciConsumptionOutput"
      }
    },
    "LciPollutionOutput": {
      "type": "object",
      "properties": {
        "co2Sum": {
          "type": "number",
          "format": "float",
          "description": "Sum of CO2"
        },
        "ch4Sum": {
          "type": "number",
          "format": "float",
          "description": "Sum of CH4"
        },
        "no2Sum": {
          "type": "number",
          "format": "float",
          "description": "Sum of NO2"
        },
        "coSum": {
          "type": "number",
          "format": "float",
          "description": "Sum of CO"
        },
        "noxSum": {
          "type": "number",
          "format": "float",
          "description": "Sum of NOx"
        },
        "nmvocSum": {
          "type": "number",
          "format": "float",
          "description": "Sum of NMVOC"
        },
        "particulatesSum": {
          "type": "number",
          "format": "float",
          "description": "Sum of particulate"
        },
        "co2eSum": {
          "type": "number",
          "format": "float",
          "description": "Sum of CO2e"
        }
      },
      "xml": {
        "name": "LciPollutionOutput"
      }
    },
    "LciOutput": {
      "type": "object",
      "properties": {
        "consumptions": {
          "$ref": "#/definitions/LciConsumptionOutput"
        },
        "pollutions": {
          "$ref": "#/definitions/LciPollutionOutput"
        }
      },
      "xml": {
        "name": "LciOutput"
      }
    },
    "ApiResponse": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "type": {
          "type": "string"
        },
        "message": {
          "type": "string"
        }
      }
    }
  },
