{
  "swagger": "2.0",
  "info": {
    "description": "This is a service for running LCA, funded by California Energy Commission",
    "version": "1.0.0",
    "title": "Life Cycle Assessment",
    "contact": {
      "email": "kkyli@ucdavis.edu"
    },
    "license": {
      "name": "MIT license",
      "url": "https://github.com/ucdavis/LCA/blob/master/LICENSE"
    }
  },
  "tags": [
    {
      "name": "LCA",
      "description": "Life Cycle Assessment (LCA)"
    }
  ],
  "paths": {
    "/lcarun": {
      "get": {
        "tags": [
          "lca"
        ],
        "summary": "Runs LCA from given input",
        "description": "",
        "operationId": "runLCA",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "technology",
            "description": "Power Generation Technology",
            "in": "query",
            "required": true,
            "type": "string",
            "enum": [
              "GPO",
              "CHP"
            ],
            "default": "CHP"
          },
          {
            "name": "diesel",
            "description": "Diesel consumption (m3/kWh electricity generated)",
            "in": "query",
            "required": true,
            "type": "number",
            "default": 1
          },
          {
            "name": "gasoline",
            "description": "Gasoline consumption (m3/kWh electricity generated)",
            "in": "query",
            "required": true,
            "type": "number",
            "default": 1
          },
          {
            "name": "jetfuel",
            "description": "Jet fuel consumption (m3/kWh electricity generated)",
            "in": "query",
            "required": true,
            "type": "number",
            "default": 1
          },
          {
            "name": "distance",
            "description": "The distance between a harvest unit and a biopower facility (miles/kWh electricity generated)",
            "in": "query",
            "required": true,
            "type": "number",
            "default": 100
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "$ref": "#/definitions/Output"
            }
          },
          "400": {
            "description": "Invalid parameters supplied"
          }
        }
      }
    }
  },
  "definitions": {
    "LciResults": {
      "type": "object",
      "properties": {
        "CO2": {
          "type": "number",
          "format": "float",
          "description": "Carbon dioxide emissions (g)",
          "example": 47882.01
        },
        "CH4": {
          "type": "number",
          "format": "float",
          "description": "Methane emissions (g)",
          "example": 1.71
        },
        "N2O": {
          "type": "number",
          "format": "float",
          "description": "Nitrous oxide emissions (g)",
          "example": 0.17
        },
        "CO2e": {
          "type": "number",
          "format": "float",
          "description": "Carbon dioxide equivalent emissions (g)",
          "example": 47974.14
        },
        "CO": {
          "type": "number",
          "format": "float",
          "description": "Carbon monoxide emissions (g)",
          "example": 192.29
        },
        "NOx": {
          "type": "number",
          "format": "float",
          "description": "Nitrogen oxides emissions (g)",
          "example": 395.03
        },
        "NMVOC": {
          "type": "number",
          "format": "float",
          "description": "Non-methane volatile organic compounds emissions (g)",
          "example": 12.09
        },
        "Particulates": {
          "type": "number",
          "format": "float",
          "description": "Particulate matter emissions (g)",
          "example": 19.82
        }
      },
      "xml": {
        "name": "LciResults"
      }
    },
    "LciaResults": {
      "type": "object",
      "properties": {
        "global_warming_air": {
          "type": "number",
          "format": "float",
          "description": "Global Warming Potentials for Air Emissions (kg CO2 eq/kg substance)",
          "example": 47.97413521
        },
        "acidification_air": {
          "type": "number",
          "format": "float",
          "description": "Acidification Potentials for Air Emissions (kg SO2 eq/kg substance)",
          "example": 0.011894749
        },
        "hh_particulate_air": {
          "type": "number",
          "format": "float",
          "description": "Human Health Particulate (and secondary particulate matter precursors) Potentials for Air Emissions (PM2.5eq/kg substance)",
          "example": 0.000229894
        },
        "eutrophication_air": {
          "type": "number",
          "format": "float",
          "description": "Eutrophication Potentials for Air Emissions (kg N eq/kg substance)",
          "example": 0.000725385
        },
        "eutrophication_water": {
          "type": "number",
          "format": "float",
          "description": "Eutrophication Potentials for Water Emissions (kg N eq/kg substance)",
          "example": 0.004766378
        },
        "ozone_depletion_air": {
          "type": "number",
          "format": "float",
          "description": "Ozone Depletion Potentials for Air Emissions (kg CFC-11 eq/kg substance)",
          "example": 0
        },
        "smog_air": {
          "type": "number",
          "format": "float",
          "description": "Smog Formation Potentials for Air Emissions (kg O3 eq/kg substance)",
          "example": 0.271093548
        }
      },
      "xml": {
        "name": "LciaResults"
      }
    },
    "Output": {
      "type": "object",
      "properties": {
        "LciResults": {
          "$ref": "#/definitions/LciResults"
        },
        "LciaResults": {
          "$ref": "#/definitions/LciaResults"
        }
      },
      "xml": {
        "name": "Output"
      }
    }
  },
  "externalDocs": {
    "description": "Source code",
    "url": "https://github.com/ucdavis/LCA"
  }
}